<h1>Login here to see your thoughts!</h1>
<h4>Or <%= link_to "Sign up", new_user_path %> here</h4>

<%= form_for(:session, url: login_path) do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Submit" %>
<% end %>
